	stAuthStreamPersisted = we.State(221)

	stAuthSuccess = we.State(300)

	stCapturePrepared        = we.State(230)
	stCaptureProcessing      = we.State(231)
	stCaptureStreamPersisted = we.State(232)

	stCancelPrepared        = we.State(240)
	stCancelProcessing      = we.State(241)
	stCancelStreamPersisted = we.State(242)

	stResumePrepared = we.State(250)

	stPrepareFailureHandling      = we.State(501) // asynchronous flow failure, transitioned to this state after consuming stream msg
	stTransactionLimitCheckFailed = we.State(502) // synchronous flow failure, transitioned to this state based on transaction-limit API resp
	stRiskCheckError              = we.State(503) // when risk check service's internal error occurs
	stRiskCheckDeny               = we.State(504) // when risk check return Deny response
	stFailurePublished            = we.State(505)
