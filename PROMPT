sg-prd-m-payment-core
select tx_id, tx_type, status from internal_transaction 
where created_at >= '2025-08-28T02:38:27Z' // created_at get from above transfer table
and created_at <= '2025-08-28T03:38:27Z' // created_at + 1 hour
and group_id = '9392fb12b6c64db18e779ae60bdf4307' // use transaction_id from above

select ref_id, tx_type, status from external_transaction 
where created_at >= '2025-08-28T02:38:27Z'
and created_at <= '2025-08-28T03:38:27Z'
and group_id = '9392fb12b6c64db18e779ae60bdf4307'

select workflow_id, run_id, state, attempt from workflow_execution 
where run_id in (
    'b4f3e1f3e1b14c2fa6f4e5b6c7d8e9f0', // use internal_transaction tx_id from above
    'c1d2e3f4a5b6478899aabbccddeeff00'  // use external_transaction ref_id from above
)

sg-prd-m-fast-adapter

SELECT type, instruction_id, status, cancel_reason_code, reject_reason_code FROM transactions where 
instruction_id='20250828OCBCSGSGBRT6958702' // use external_id from transfer table above
and created_at >= '2025-08-28T02:38:27Z' // created_at get from above transfer table
and created_at <= '2025-08-28T03:38:27Z' // created_at + 1 hour

